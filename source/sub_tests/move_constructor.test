
TEST_CASE("move constructor", "[constructor]") {
    List <int> list;
    list.push_front(1);
    list.push_front(2);
    list.push_front(3);
    list.push_front(4);

    List<int> list2 = std::move(list);
    
    REQUIRE(0 == list.size());
    REQUIRE(list.empty());
    REQUIRE(4 == list2.size());
}